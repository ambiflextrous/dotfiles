# Vim Keys
def_key "k"
   scroll_up

def_key "j"
   scroll_down

def_key "l"
    next_column

def_key "h"
    previous_column

def_key "ctrl-u"
    page_up

def_key "ctrl-d"
    page_down

def_key "i"
    add_item_to_playlist

def_key "x"
    delete_playlist_items

def_key "9"
    volume_up

def_key "0"
    volume_down

def_key "c"
    show_clock

def_key "ctrl-x"
    clear_playlist

def_key "s"
    shuffle

def_key "q"
    quit
